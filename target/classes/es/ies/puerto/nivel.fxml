      </Button>
      <Button fx:id="buttonRegresar" minWidth="165.0" mnemonicParsing="false" onAction="#buttonRegresarClick" text="Regresar">
         <VBox.margin>
            <Insets top="60.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
